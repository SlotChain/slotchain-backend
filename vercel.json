{
  "version": 2,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/index.js"
    },
    {
      "src": "/favicon.ico",
      "status": 404
    },
    {
      "src": "/(.*)",
      "status": 404
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "https://slotchain-app.vercel.app" },
        { "key": "Access-Control-Allow-Credentials", "value": "true" },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET,OPTIONS,POST,PUT,PATCH,DELETE"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Origin,Content-Type,Accept,Authorization"
        }
      ]
    }
  ],
  "functions": {
    "api/index.js": {
      "maxDuration": 60,
      "memory": 1024,
      "includeFiles": ["dist/**"]
    }
  },
  "env": {
    "NODE_ENV": "production"
  }
}
